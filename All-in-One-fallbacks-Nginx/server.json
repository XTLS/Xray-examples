    //   }
    // }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "protocol": "blackhole",
      "tag": "blackhole"
    },
    {
      // A DNS Cache can be setup and added here to imporve performance (the corresponding rule should be uncommented)
      "tag": "DNS-Internal",
      "protocol": "dns",
      "settings": {
        "address": "127.0.0.53",
        "port": 53
      }
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "settings": {
      "rules": [
        // {
        //   "type": "field",
        //   "outboundTag": "blackhole",
        //   "ip": ["geoip:private"]
        // },
        // { DNS Cache rule
        //   "type": "field",
        //   "port": 53,
        //   "network": "tcp,udp",
        //   "outboundTag": "DNS-Internal"
        // },
        {
          "type": "field", // Block BitTorrent protocol
          "outboundTag": "blackhole",
          "protocol": ["bittorrent"]
        }
      ]
    }
  }
