{
    "log": {
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 10800,
            "listen": "127.0.0.1",
            "protocol": "socks",
            "settings": {
                "udp": true
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "vmess",
            "settings": {
                "vnext": [
                    {
                        "address": "example.com", // Replace it with your domain name or server IP (there is no need to resolve the domain name when making a requestï¼‰
                        "port": 443,
                        "users": [
                            {
                                "id": "", // fill in your UUID
                                "security": "none",
                                "level": 0
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "example.com" // Change to your domain name
                },
                "wsSettings": {
                    "path": "/vmessws" // It must be replaced with a custom PATH, which needs to be consistent with that of the server
                }
            }
        }
    ]
}
