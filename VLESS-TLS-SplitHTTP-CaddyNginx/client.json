{
    "log": {
      "loglevel": "debug"
    },
    "inbounds": [
      {
        "listen": "127.0.0.1",
        "port": 10808,
        "protocol": "socks"
      }
    ],
    "outbounds": [
      {
        "protocol": "vless",
        "settings": {
          "vnext": [
            {
              "address": "",
              "port": 443,
              "users": [
                {
                  "id": "",
                  "encryption": "none"
                }
              ]
            }
          ]
        },
        "streamSettings": {
          "network": "splithttp",
          "splithttpSettings": {
            "path": "/split"
          },
          "security": "tls",
          "tlsSettings": {
            "serverName": ""
          }
        },
        "tag": "proxy"
      }
    ]
  }
  
